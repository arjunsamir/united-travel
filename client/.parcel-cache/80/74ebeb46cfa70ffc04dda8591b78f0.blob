r(--spinner-style) var(--preload-color-alt);\n        border-right: var(--spinner-style) var(--preload-color-alt);\n        border-radius: 50%;\n\n        transition: border $smooth ease;\n        animation: preloadSpin 1.2s $cubic infinite;\n    }\n\n}\n\n\n.page-transition-slide {\n\n    --preload-color: var(--jet);\n    --preload-color-alt: #{rgba($jet, .5)};\n    --spinner-style: 2px solid;\n    --spinner-size: 20rem;\n    \n    @include flexCenter;\n    height: 100%;\n    width: 100%;\n\n    position: fixed;\n    top: 0;\n    left: 0;\n    z-index: 1000;\n\n    transform: translateY(100vh);\n\n    background-color: var(--primary);\n\n}",".navbar {\n\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n\n    position: fixed;\n    top: 0;\n    left: 0;\n    z-index: 100;\n\n    width: 100vw;\n\n    padding: 4rem var(--x-pad);\n\n    * { user-select: none; }\n\n    // Define Colors\n    --color-brand-default: var(--white);\n    --color-links-default: var(--white);\n\n    --color-brand-tablet: var(--color-brand-default);\n    --color-links-tablet: var(--color-links-default);\n\n    --color-brand-mobile: var(--color-brand-tablet);\n    --color-links-mobile: var(--color-links-tablet);\n\n    --color-brand-dark: var(--white);\n    --color-links-dark: var(--white);\n\n\n    // Apply Colors\n    --nav-text-color: var(--color-links-default);\n    --nav-brand-color: var(--color-brand-default);\n\n    @include ipadPro {\n        --nav-text-color: var(--color-links-tablet);\n        --nav-brand-color: var(--color-brand-tablet);\n    }\n\n    @include ipad {\n        --nav-text-color: var(--color-links-mobile);\n        --nav-brand-color: var(--color-brand-mobile);\n    }\n\n    @include dark {\n        --nav-text-color: var(--color-links-dark);\n        --nav-brand-color: var(--color-brand-dark);\n    }\n\n    transition: background $smooth ease, transform $smooth $cubic;\n\n    transform: translate3d(0,0,0);\n\n    &.hidden {\n        transform: translate3d(0, -100%, 0);\n    }\n\n    &.opaque {\n        background-color: var(--black);\n    }\n\n\n    &__main {\n        display: flex;\n        align-items: center;\n\n        @include respond(tab-port) {\n            display: none;\n        }\n    }\n\n\n    &__mobile {\n\n        display: none;\n\n        @include respond(tab-port) {\n            display: block;\n        }\n\n        position: fixed;\n        top: 0;\n        left: 0;\n\n        overflow-y: scroll;\n\n        height: 100vh;\n        width: 100vw;\n\n        transform: translate3d(100vw, 0, 0);\n\n        background-color: white;\n\n        transition: transform $smooth $cubic, box-shadow $normal ease;\n\n\n        &-account {\n            padding: 5rem var(--x-pad) 4rem var(--x-pad);\n\n            background-color: var(--bg-dark-alt);\n\n            .logged-in {\n            \n                &__grid {\n                    display: grid;\n                    grid-template-columns: max-content 1fr;\n                    grid-gap: 2rem;\n                    align-items: center;\n                }\n\n                padding: 2rem 0;\n\n                &, &:link, &:visited {\n                    color: var(--text-light);\n                }\n\n                h4 {\n                    color: var(--white);\n                    margin-bottom: .7rem;\n                }\n\n                .carat-btn {\n                    margin-top: 3rem;\n                    margin-left: 10rem;\n                    @include link {\n                        color: var(--white);\n                    }\n                }\n            }\n\n            .logged-out {\n                padding-top: 3rem;\n\n                h3 {\n                    margin-bottom: 2rem;\n                    color: var(--white);\n                }\n\n                a {\n                    display: block;\n                    width: max-content;\n\n                    &:not(:last-child) {\n                        margin-bottom: 1rem;\n                    }\n                }\n\n            }\n        }\n\n        &-profile-photo {\n            height: 8rem;\n            width: 8rem;\n            border-radius: 50%;\n            overflow: hidden;\n\n            img {\n                display: block;\n                width: 100%;\n            }\n        }\n\n        &-links {\n            padding: 4rem var(--x-pad) 0 var(--x-pad);\n\n            ul {\n                li {\n                    margin: 1rem 0;\n\n                }\n            }\n\n            a {\n                display: block;\n                font-size: 2.1rem;\n                font-weight: 300;\n                padding: 1.5rem 0;\n\n                @include link {\n                    color: var(--text-dark);\n                }\n\n                &.active {\n                    @include link {\n                        color: var(--primary);\n                    }\n                }\n            }\n        }\n\n        &-language {\n            padding: 0 var(--x-pad) 8rem var(--x-pad);\n\n            hr {\n                margin-top: 2rem;\n                margin-bottom: 3.5rem;\n            }\n\n            ul {\n                margin-top: 2rem;\n                display: flex;\n\n                li {\n                    display: block;\n\n                    font-size: 2rem;\n\n                    &:last-child {\n                        margin-left: 3rem;\n                    }\n                    \n                    &:not(.active) a {\n                        &, &:link, &:visited {\n                            color: var(--text-light);\n                        }\n                    }\n                }\n            }\n        }\n\n\n    }\n\n\n    &__checkbox {\n        display: none;\n    }\n\n\n    &__checkbox:checked + &__mobile {\n        transform: translate3d(0, 0, 0);\n        z-index: -1;\n    }\n\n\n    &__icon {\n\n        height: 2.6rem;\n        width: 2.6rem;\n        position: relative;\n\n        &.open {\n
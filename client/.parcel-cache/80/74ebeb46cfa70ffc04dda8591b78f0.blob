) {\n    @if $breakpoint == iphone {\n        @media(max-width: 23em) { @content }; //400px\n    }\n    @if $breakpoint == phone {\n        @media(max-width: 37.5em) { @content }; //600px\n    }\n    @if $breakpoint == tab-port {\n        @media(max-width: 56.25em) { @content }; //900px\n    }\n    @if $breakpoint == tab-land {\n        @media(max-width: 75em) { @content }; //1200px\n    }\n    @if $breakpoint == laptop {\n        @media(max-width: 85em) { @content }; //1400px\n    }\n    @if $breakpoint == desktop {\n        @media(max-width: 95em) { @content }; // 1520\n    }\n}\n\n@mixin iphone {\n    @media(max-width: 23em) { @content }; //400px\n}\n\n@mixin phone {\n    @media(max-width: 37.5em) { @content }; //600px\n}\n\n@mixin ipad {\n    @media(max-width: 56.25em) { @content }; //900px\n}\n\n@mixin ipadPro {\n    @media(max-width: 75em) { @content }; //1200px\n}\n\n@mixin laptop {\n    @media(max-width: 85em) { @content }; //1400px\n}\n\n@mixin responsive($breakpoint) {\n    @media(max-width: $breakpoint) { @content }; //900px\n}\n\n\n@mixin light {\n    @media( prefers-color-scheme: light ) { @content }; // Light Theme\n}\n\n@mixin dark {\n    @media( prefers-color-scheme: dark ) { @content }; // Dark Theme\n}\n","// Color Variables\n$primary: #EEB559;\n$jet: #333333;\n$text-dark: #999999;\n$text-light: #CFCFCF;\n$white: #FFFFFF;\n$black: #000000;\n$bg-dark: #121212;\n$bg-dark-alt: #222222;\n$bg-light: #FFFFFF;\n$bg-light-alt: #F9F9F9;\n\n$error-color: #D45555;\n$success-color: #80CF9B;\n\n$google-color: #4285F4;\n$facebook-color: #1978F2;\n\n\n:root {\n    --primary: #{$primary};\n    --text-dark: #{$text-dark};\n    --text-light: #{$text-light};\n    --white: #{$white};\n    --black: #{$black};\n    --jet: #{$jet};\n    --bg-dark: #{$bg-dark};\n    --bg-dark-alt: #{$bg-dark-alt};\n    --bg-light: #{$white};\n    --bg-light-alt: #{$bg-light-alt};\n    --card-dark: #363636;\n    --error-color: #{$error-color};\n    --success-color: #{$success-color};\n    
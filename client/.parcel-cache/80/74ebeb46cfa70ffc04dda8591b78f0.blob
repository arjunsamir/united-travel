 cursor: pointer;\n                \n                &:focus {\n                    outline: none;\n                }\n            }\n\n        }\n\n        svg {\n            stroke: var(--inner-color);\n            height: var(--icon-size);\n            width: var(--icon-size);\n            position: absolute;\n            pointer-events: none;\n            top: calc(50% - var(--icon-size) / 2);\n            right: var(--icon-size);\n            opacity: 0;\n\n            &.toggle {\n                opacity: 1;\n                cursor: pointer;\n                pointer-events: all;\n                stroke: var(--jet);\n                transition: opacity $dt;\n                @include dark {\n                    stroke: var(--white);\n                }\n\n                &:hover {\n                    opacity: .75;\n                }\n\n                &.visible {\n                    stroke: transparent;\n                    fill: var(--jet);\n                    @include dark {\n                        stroke: var(--white);\n                    }\n                }\n            }\n        }\n\n        &:last-child svg {\n            right: calc(var(--icon-size) - var(--input-padding));\n            @include phone { right: var(--icon-size); }\n        }\n\n        &.valid svg:last-child, &.error svg:not(:last-child) { opacity: 1; }\n\n    }\n\n    &__tooltip {\n\n        --tt-bg: var(--jet);\n\n        @include dark { --tt-bg: var(--text-dark); }\n        \n        position: absolute;\n        top: -2rem;\n        left: 0;\n        transform: translateY(-100%);\n\n        padding: 1rem;\n        background-color: var(--tt-bg);\n        color: var(--white);\n        pointer-events: none;\n\n        transition: opacity $dt;\n\n        p {\n            font-size: $h6;\n        }\n\n        &::after {\n            @include pseudo;\n            height: 2rem;\n            width: 2rem;\n\n            bottom: -1rem;\n            right: 1rem;\n            transform: rotate(45deg);\n            background-color: var(--tt-bg);\n        }\n\n        &.hidden {\n            opacity: 0;\n        }\n    }\n\n}\n\n\n.textarea {\n\n    --textarea-color: var(--jet);\n    --heading-text: var(--text-dark);\n\n    padding: 1rem 3rem;\n    border: 2px solid currentColor;\n\n    display: flex;\n    flex-direction: column-reverse;\n    position: relative;\n\n    transition: all $dt;\n\n    color: var(--textarea-color);\n\n    @include phone {\n        padding: 1rem 2rem;\n    }\n\n    @include dark {\n        --textarea-color: var(--white);\n        --heading-text: var(--text-light);\n    }\n\n    &:focus-within {\n        border-color: var(--primary);\n        --heading-text: var(--primary);\n    }\n\n    &.valid {\n        border-color: var(--success-color);\n        &:focus-within {\n            border-color: var(--success-color);\n            --heading-text: var(--success-color);\n        }\n    }\n\n    &.error {\n        border-color: var(--error-color);\n        &:focus-within {\n            border-color: var(--error-color);\n            --heading-text: var(--error-color);\n        }\n    }\n\n    h6 {\n        color: var(--text-dark);\n        transition: all $dt;\n        margin-bottom: .5rem;\n        color: var(--heading-text);\n    }\n\n    textarea {\n\n        background: transparent;\n        border: none;\n        padding: .5rem 0;\n\n        color: currentColor;\n\n        width: 100%;\n        height: 20rem;\n\n        resize: none;\n    }\n\n}\n\n.select-tile {\n\n    display: flex;\n    cursor: pointer;\n\n    input {\n        display: none;\n    }\n\n    &__container {\n\n        padding: 2rem 1.5rem;\n        border: 2px solid var(--text-light);\n\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n\n        --h-col: var(--jet);\n        --p-col: var(--text-dark);\n        --b-col: var(--jet);\n\n        --h-col-focus: var(--white);\n        --p-col-focus: rgba(255, 255, 255, .7);\n\n  
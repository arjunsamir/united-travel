r: var(--accent-color);\n        --lt-color-alt: var(--white);\n    }\n\n    &.lt-light,\n    &.lt-hover-light:hover,\n    &.lt-hover-light:focus {\n        --lt-color: var(--white);\n        --lt-color-alt: var(--jet);\n    }\n\n    &.lt-dark,\n    &.lt-hover-dark:hover,\n    &.lt-hover-dark:focus {\n        --lt-color: var(--jet);\n        --lt-color-alt: var(--white);\n    }\n\n    &.lt-primary-text,\n    &.lt-hover-primary-text:hover,\n    &.lt-hover-primary-text:focus {\n        --lt-color-alt: var(--primary);\n    }\n\n\n\n    // Dark Color Themes\n    &.dk-primary,\n    &.dk-hover-primary:hover,\n    &.dk-hover-primary:focus {\n        --dk-color: var(--primary);\n        --dk-color-alt: var(--jet);\n    }\n\n    &.dk-accent,\n    &.dk-hover-accent:hover,\n    &.dk-hover-accent:focus {\n        --dk-color: var(--accent-color);\n        --dk-color-alt: var(--jet);\n    }\n\n    &.dk-light,\n    &.dk-hover-light:hover,\n    &.dk-hover-light:focus {\n        --dk-color: var(--white);\n        --dk-color-alt: var(--jet);\n    }\n\n    &.dk-dark,\n    &.dk-hover-dark:hover,\n    &.dk-hover-dark:focus {\n        --dk-color: var(--jet);\n        --dk-color-alt: var(--white);\n    }\n\n    &.dk-primary-text,\n    &.dk-hover-primary-text:hover,\n    &.dk-hover-primary-text:focus {\n        --dk-text: var(--primary);\n    }\n\n\n    // Opacity Hover\n    &.opacity-75,\n    &.lt-opacity-75,\n    &.hover-opacity-75:hover,\n    &.lt-hover-opacity-75:hover,\n    &.hover-opacity-75:focus,\n    &.lt-hover-opacity-75:focus {\n        --lt-opacity: .75;\n    }\n\n    &.opacity-50,\n    &.lt-opacity-50,\n    &.hover-opacity-50:hover,\n    &.lt-hover-opacity-50:hover,\n    &.hover-opacity-50:focus,\n    &.lt-hover-opacity-50:focus {\n        --lt-opacity: .5;\n    }\n    \n    &.dk-opacity-75,\n    &.dk-hover-opacity-75:hover,\n    &.dk-hover-opacity-75:focus {\n        --dk-opacity: .75;\n    }\n\n    &.dk-opacity-50,\n    &.dk-hover-opacity-50:hover,\n    &.dk-hover-opacity-50:focus {\n        --dk-opacity: .5;\n    }\n\n    \n\n}\n\n\n.btn {\n\n    display: block;\n    min-width: 20rem;\n    width: max-content;\n    padding: 1.5rem 3rem;\n\n    text-transform: uppercase;\n    letter-spacing: $spacing;\n    font-weight: 700;\n    font-size: 1.4rem;\n    text-align: center;\n    line-height: 1.7;\n\n    border-width: 2px;\n    border-style: solid;\n\n    cursor: pointer;\n    transition: all $dt;\n\n\n    // Button Modifiers\n    &.wide { width: 100%; }\n\n    &.hollow,\n    &.hover-hollow:hover,\n    &.hover-hollow:focus,\n    &.lt-hollow,\n    &.lt-hover-hollow:hover,\n    &.lt-hover-hollow:focus {\n        --lt-text: var(--lt-color);\n        --lt-bg: transparent;\n        --lt-border: currentColor;\n    }\n\n    &.dk-hollow,\n    &.dk-hover-hollow:hover,\n    &.dk-hover-hollow:focus {\n        --dk-text: var(--dk-color);\n        --dk-bg: transparent;\n        --dk-border: currentColor;\n    }\n\n    &.filled,\n    &.hover-fill:hover,\n    &.hover-fill:focus {\n        --dk-text: var(--dk-color-alt);\n        --dk-bg: var(--dk-color);\n        --dk-border: var(--dk-color);\n    }\n\n    &.filled,\n    &.hover-fill:hover,\n    &.hover-fill:focus,\n    &.lt-filled,\n    &.lt-hover-fill:hover,\n    &.lt-hover-fill:focus {\n        --lt-bg: var(--lt-color);\n        --lt-border: var(--lt-color);\n        --lt-text: var(--lt-color-alt);\n    }\n\n    &.dk-filled,\n    &.dk-hover-fill:hover,\n    &.dk-hover-fill:focus {\n        --dk-bg: var(--dk-color);\n        --dk-border: var(--dk-color);\n        --dk-text: var(--dk-color-alt);\n    }\n\n}","@mixin link {\n    &, &:link, &:visited { @content };\n}\n\n@mixin uppercase {\n    text-transform: uppercase;\n    letter-spacing: .1em;\n}\n\n@mixin flexCenter {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n\n@mixin overlay {\n    position: absolute;\n    top: 0;\n    left: 0;\n    \n    height: 100%;\n    width: 100%;\n}\n\n@mixin p
        </section>
        
        <!-- Contact Us -->
        <section id="contact" class="contact">
    <div class="contact__image">
        <picture>
            <source srcset="https://storage.googleapis.com/utravel-site-content/img/contact.webp" type="image/webp">
            <source srcset="https://storage.googleapis.com/utravel-site-content/img/contact.jpg" type="image/jpeg">
            <img src="https://storage.googleapis.com/utravel-site-content/img/contact.jpg" alt="Mercedes car coming out of a circle">
        </picture>
    </div>
    <div class="contact__card" data-scroll data-scroll-speed="2">
        <div class="content">
            <h2>Have a question? <span>Contact us.</span></h2>
            <p>We’re happy to help you! Please feel free to give us a call or send us an email for any questions or concerns. We will get back to you as soon as possible.</p>
            <a class="contact__method" href="mailto:<%- settings.contact.email %>" >
                <svg><use href="/img/icons.svg#email"></use></svg>
                <h5><%- settings.contact.email %></h5>
            </a>
            <a class="contact__method" href="tel:<%- settings.contact.phone %>">
                <svg><use href="/img/icons.svg#iphone"></use></svg>
                <h5><%- settings.phone %></h5>
            </a>
        </div>
    </div>
</section>

        
        <!-- Footer -->
        <footer class="footer" id="footer">

    <div class="footer__brands">
        <div class="footer__brand">
            <svg><use href="/img/icons.svg#logo"></use></svg>
            <p>United Travel LLC</p>
        </div>
        <a href="https://www.arjunsamir.com" target="_blank" class="footer__agency">Designed by Day FiftyFive</a>
    </div>

    <div class="footer__main">
        <div class="footer__content">
            <div class="footer__contact">
                <h5 class="white">Contact Us</h5>
                <hr>
                <a href="mailto:<%- settings.contact.email %>"><%- settings.contact.email %></a>
                <a href="tel:<%- settings.contact.phone %>"><%- settings.phone %></a>
            </div>
            <ul class="footer__links">
                <li><a href="/account">My Account</a></li>
                <li><a href="/privacy">Privacy Policy</a></li>
                <li><a href="/terms">Terms of Service</a></li>
            </ul>
        </div>
        <div class="footer__copyright">
            <p>&copy; <%- settings.copyrightYear %> United Travel LLC. All rights reserved.</p>
        </div>
    </div>

</footer>

    </main>
    
    <!-- JavaScript -->
    <!-- Decalre Global Locale -->
<script>
    window.locale = "en";
    window.currentUser = JSON.parse('<%- JSON.stringify(user) %>');
    window.settings = JSON.parse('<%- JSON.stringify(settings) %>');
</script>

<!-- External JavaScript Links -->
<script src="/js/index.js"></script> 

</body>

</html>